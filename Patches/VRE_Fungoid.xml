<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Races Expanded - Fungoid</li>
    </mods>
    <match Class="PatchOperationAddModExtension">
      <xpath>Defs/GeneDef[defName="VRE_MindCoalescence"]</xpath>
      <value>
        <li Class = "NeedBarOverflow.DisableNeedOverflowExtension">
          <disableOverflowNeeds>
            <li>Need_Mood</li>
          </disableOverflowNeeds>
        </li>
      </value>
    </match>
  </Operation>
</Patch>
